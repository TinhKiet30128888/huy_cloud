
<%- include('parts/header') %>



<div class="container">
         <h1>update form</h1>
    <form  method="post" action="/update/<%-updatedata._id %>" >
  
      <div class="form-group">
        <label for="exampleInputPassword1">Name</label>
        <input type="text" class="form-control" name="name" value="<%- updatedata.name %>">
      </div>
  
      <div class="form-group">
        <label for="exampleInputPassword1">Email</label>
        <input type="email" class="form-control" name="email" value="<%- updatedata.email %>">
      </div>
      
      <div class="form-group">
        <label for="exampleInputPassword1">Phone</label>
        <input type="number" class="form-control" name="phone" value="<%- updatedata.phone %>">
      </div>
      
  
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>



<%- include('parts/footer') %>
  